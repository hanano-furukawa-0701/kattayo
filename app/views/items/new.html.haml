-# .header
-#   = render "home/header"

.items-new
  .items-new__title
    %h2 購入品登録
  .items-new__form
    = form_with url: items_path, method: :post do |f|
      .items-new__form__name
        = f.label :購入品名, class: 'items-new__form__name__label'
      .items-new__form__category
        = f.label :カテゴリー, class: 'items-new__form__category__label'
      .items-new__form__price
        = f.label :価格, class: 'items-new__form__price__label'
      .items-new__form__text
        = f.label :メモ, class: 'items-new__form__text__label'
  